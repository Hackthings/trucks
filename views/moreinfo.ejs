<!DOCTYPE html>
<% include sign-header.ejs %>
    <div class="container log-form" id="register-form">
      <h2>Register Form</h2>
      <form method="post" action="/moreinfo/:username">
<!--        <% if(typeof(errorMessage) !== 'undefined') {%>
       <span><%= errorMessage %></span>
       <% } %> -->
       <fieldset class="fieldset">

       <hr>
       <p>
        <label class="register-fname" for="">First Name: </label>
        <input type="text" name="fname" id=""
           placeholder="" required="true"/>
       </p>
       <p>
        <label class="register-lname" for="">Last Name: </label>
        <input type="text" name="lname" id=""
           placeholder="" required="true"/>
       </p>
       <p>
        <label class="register-address" for="">Address: </label>
        <input type="text" name="registerAddress" id=""
           placeholder="" required="true"/>
       </p>
       <p>
        <label class="register-city"for="">City: </label>
        <input type="text" name="registerCity" id=""
           placeholder="" required="true"/>
       </p>
       <p>
        <label class="register-state"for="">State: </label>
        <input type="text" name="registerState" id=""
           placeholder="" required="true"/>
       </p>
       <p>
        <label class="register-zip"for="zip">Zip: </label>
        <input type="text" name="registerZip" id=""
           placeholder="" required="true"/>
       </p>
       <p>
        <label class="register-phone" for="">Phone: </label>
        <input type="text" name="registerPhone" id=""
           placeholder="" required="true"/>
       </p>
       <p>
        <label class="register-email" for="username">Email: </label>
        <input type="text" name="registerEmail" id="username"
           placeholder="" required="true"/>
       </p>

       <p>
        <label class="register-license" for="">License number: </label>
        <input type="text" name="stateLicense" id=""
           placeholder="" required="true"/>
       </p>
       <p>
        <label class="register-state" name="stateLicenseState" for="">Issuing State: </label>
