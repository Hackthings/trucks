<!DOCTYPE html>
<% include header.ejs %>
    <div class="container">
      <div class="row">
        <div class="col-md-6 col-md-offset-3 personal-info">
          <h3 class="col-md-offset-5 form-label">Patient info</h3>
          <form role="form" class="form-horizontal  action="/demographics" method="post">
            <div class="form-group">
              <label class="col-lg-3 control-label">First name:</label>
              <div class="col-lg-8">
                <input type="text" placeholder="First name" class="form-control fName" name="fName"/>
              </div>
            </div>
            <div class="form-group">
              <label class="col-lg-3 control-label">Middle name:</label>
              <div class="col-lg-8">
                <input type="text" placeholder="Middle Name" class="form-control mName" name="mName"//>
              </div>
            </div>
            <div class="form-group">
              <label class="col-lg-3 control-label">Last name:</label>
              <div class="col-lg-8">
                <input type="text" placeholder="Last name" class="form-control lName" name="lName"//>
              </div>
            </div>
            <div class="form-group">
              <label class="col-lg-3 control-label">Birthday:</label>
              <div class="col-lg-8">
                <input type="text" placeholder="12/10/1976" class="form-control dob" name="dob"//>
              </div>
            </div>
            <div class="form-group">
              <label class="col-lg-3 control-label">SSN:</label>
              <div class="col-lg-8">
                <input type="text" placeholder="123-45-6789" class="form-control ssn" name="ssn"//>
              </div>
            </div>
            <div class="form-group">
              <label class="col-lg-3 control-label">Phone:</label>
              <div class="col-lg-8">
                <input type="text" placeholder="(555)-555-5555" class="form-control phone" name="phone"//>
              </div>
            </div>
            <div class="form-group">
              <label class="col-lg-3 control-label">Address:</label>
              <div class="col-lg-8">
                <input type="text" placeholder="123 Main Street" class="form-control address" name="address"//>
              </div>
            </div>
            <div class="form-group">
              <label class="col-lg-3 control-label">City:</label>
              <div class="col-lg-8">
                <input type="text" placeholder="City" class="form-control city" name="city"//>
              </div>
            </div>
            <div class="form-group">
              <label class="col-lg-3 control-label">State:</label>
              <div class="col-lg-8">
                <input type="text" placeholder="123 Main Street" class="form-control state" name="state"//>
              </div>
            </div>
            <div class="form-group">
              <label class="col-lg-3 control-label">Zip:</label>
              <div class="col-lg-8">
                <input type="text" placeholder="12345" class="form-control zip" name="zip"//>
              </div>
            </div>
            <div class="form-group">
              <label class="col-lg-3 control-label">Country:</label>
              <div class="col-lg-8">
                <input type="text" placeholder="US" class="form-control zip" name="country"//>
              </div>
            </div>
            <div class="form-group">
              <label class="col-lg-3 control-label">Email:</label>
              <div class="col-lg-8">
                <input type="text" placeholder="email@mail.com" class="form-control email" name="email"//>
              </div>
            </div>
            <div class="form-group">
              <label class="col-lg-3 control-label">Company:</label>
              <div class="col-lg-8">
                <input type="text" placeholder="Company" class="form-control company" name="company"//>
              </div>
            </div>
            <div class="form-group hidden">
              <label class="col-lg-3 control-label">Test</label>
              <div class="col-lg-8">
                <input type="text" placeholder="First name" class="form-control fName" name="username" value="<%= user.username %>" />
              </div>
            </div>
            <div class="form-group"></div>
            <div class="form-group">
              <label class="col-md-3 control-label"></label>
              <div class="col-md-8">
                <input type="submit" value="Save Changes" class="btn btn-primary submitBtn"/><span></span>
                <input type="reset" value="Clear form" class="btn btn-default clearBtn"/>
              </div>
            </div>
          </form>
        </div>
      </div>
    </div>
<% include footer.ejs %>
